{
  "name": "zenkit-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI toolkit for web developers, featuring icon management and more.",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "zenkit": "dist/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenkiet/zenkit.git"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "node dist/index.mjs",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "cli",
    "icons",
    "toolkit",
    "zen",
    "web",
    "development"
  ],
  "author": "ZenKiet",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/prompt-cli": "^20.2.0",
    "@commitlint/types": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@iconify/tools": "^5.0.0",
    "cac": "^6.7.14",
    "picocolors": "^1.1.1"
  }
}
